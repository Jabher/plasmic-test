{
  "root": true,
  "extends": [
    "@lidofinance",
    "prettier"
  ],
  "plugins": ["typescript-sort-keys"],
  "rules": {
    "import/order": "error",
    "import/namespace": "off",
    "react/no-unknown-property": [
      2,
      {
        "ignore": [
          "jsx"
        ]
      }
    ]
  }
}
